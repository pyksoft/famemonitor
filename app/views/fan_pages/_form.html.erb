<%= simple_form_for(@fan_page) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :celebrity_id %>
    <%= f.input :user_id %>
    <%= f.input :url %>
    <%= f.input :name %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
